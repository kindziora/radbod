{
  "name": "radbod",
  "version": "1.0.0",
  "description": "framework for next gen ui ",
  "module": "./build/app.js",
  "main": "./build/app.js",
  "browser": "./dist/radbod.js",
  "type": "module",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "dev": "npx webpack --watch --entry ./build/app.js --output-filename radbod.js --mode development --output-library radbod",
    "prod": "npx webpack --watch --entry ./build/app.js --output-filename radbod.js --mode production --output-library radbod",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kindziora/radbod.git"
  },
  "keywords": [
    "next",
    "gen",
    "ts",
    "ui"
  ],
  "author": "alexander kindziora",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kindziora/radbod/issues"
  },
  "homepage": "https://github.com/kindziora/radbod#readme",
  "devDependencies": {
    "@types/expect-puppeteer": "^4.4.7",
    "@types/jest-environment-puppeteer": "^4.4.1",
    "@types/puppeteer": "^5.4.4",
    "browser-env": "^3.3.0",
    "jest": "^27.2.5",
    "jest-puppeteer": "^6.0.0",
    "puppeteer": "latest",
    "systemjs": "latest",
    "typescript": "latest",
    "webpack": "^5.54.0",
    "webpack-cli": "^4.8.0"
  }
}
